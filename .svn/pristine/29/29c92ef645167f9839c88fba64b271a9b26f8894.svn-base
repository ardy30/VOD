public interface RequestCode {

    /**
     * 系统错误
     */
    String SYSTEM_ERROR = "E000000";

    /**
     * 响应成功
     */
    String RESPONSE_SUCCESS = "N000000";

    /**
     * 找不到请求
     */
    String REQUEST_NOT_FOUND = "E000004";

    /**
     * 列表内容为空
     */
    String CONTENT_EMPTY = "N000004";

    /**
     * MAC为空
     */
    String MAC_EMPTY = "E000101";

    /**
     * MAC格式不合法
     */
    String MAC_WRONGFUL = "E000102";

    /**
     * SN为空
     */
    String SN_EMPTY = "E000103";

    /**
     * SN格式不合法
     */
    String SN_WRONGFUL = "E000104";

    /**
     * 设备类型为空
     */
    String DEVICE_TYPE_EMPTY = "E000105";

    /**
     * 用户不存在
     */
    String USER_NOT_EXIST = "E000106";

    /**
     * 手机号码为空
     */
    String PHONE_EMPTY = "E000107";

    /**
     * 手机号码格式不合法
     */
    String PHONE_WRONGFUL = "E000108";

    /**
     * 验证码类型不合法
     */
    String VERIFICATION_CODE_WRONGFUL = "E000109";

    /**
     * 操作类型不合法
     */
    String OPERATION_TYPE_WRONGFUL = "E000110";

    /**
     * 设备类型不合法
     */
    String DEVICE_TYPE_WRONGFUL = "E000111";

    /**
     * 验证码为空
     */
    String VERIFICATION_CODE_EMPTY = "E000112";

    /**
     * 用户ID为空
     */
    String USER_ID_EMPTY = "E000113";

    /**
     * 验证码错误
     */
    String VERIFICATION_CODE_ERROR = "E000114";

    /**
     * 用户名为空
     */
    String USER_NAME_EMPTY = "E000115";

    /**
     * 新用户名为空
     */
    String NEW_USER_NAME_EMPTY = "E000116";

    /**
     * 新旧用户名相同
     */
    String SAME_USER_NAME = "E000117";

    /**
     * 该用户名已存在
     */
    String USER_NAME_EXIST = "E000118";

    /**
     * 已经修改过用户名
     */
    String USER_NAME_REVISED = "E000119";

    /**
     * 密码错误
     */
    String PASSWORD_ERROR = "E000125";

    /**
     * token认证失败
     */
    String TOKEN_VERIFICATION_ERROR = "E000138";

//    E000120 第一次输入密码为空
//    E000121 第二次数据密码为空
//    E000122 两次输入密码不一致
//    E000123 token失效
//    E000124 操作类型为空
//    E000126 原密码错误
//    E000127 该手机已经被绑定
//    E000128 ID对应用户名错误
//    E000129 字段缺失
//    E000130 设备码格式不合法
//    E000131 余额为空
//    E000132 密码为空
//    E000133 用户未设置密码
//    E000134 p不能为空
//    E000135 t不能为空
//    E000136 k不能为空
//    E000137 p值不合法
//    E000139 用户名格式不合法
//    E000140 用户钱包已冻结

}
